{% extends "../../common/layout.pug" %}

block header
        i.fa-fw.fa.fa-signal
        | PX4 - Motor Control

block main
    .row.row-fluid
      article.col-xs-10.col-sm-6.col-md-6.col-lg-6(style="max-width:450px")
        #wid-SA.jarviswidget.jarviswidget-color-blueDark(data-widget-editbutton='false')
          header(style="overflow:hidden;")
            span.widget-icon
              i.fa.fa-table
            h2 Actuator Armed
          div
            .jarviswidget-editbox
              input.form-control(type='text')
            .widget-body
              .table-responsive
                table.table.table-condensed
                  tbody
                    tr
                      th Time (Seconds)
                      td
                        span(data-sage="{'tlm': [{'name': '/CFS/PX4/AA_TimeSec'}]}") ---
                    tr
                      th Time (Subseconds)
                      td
                        span(data-sage="{'tlm': [{'name': '/CFS/PX4/AA_TimeSubSec'}]}") ---
                    tr
                      th Armed
                      td
                        span(data-sage="{'tlm': [{'name': '/CFS/PX4/AA_Armed'}]}") ---
                    tr
                      th Prearmed
