extends ../../common/layout

block header
        i.fa-fw.fa.fa-signal
        | Data Storage

block main
  .row
    article.col-xs-12.col-sm-12.col-md-6.col-lg-4(style="width:350px")
      #wid-DS-General.jarviswidget.jarviswidget-color-blueDark(data-widget-editbutton='false')
        header
          span.widget-icon
            i.fa.fa-table
          h2 Housekeeping
        div
          .jarviswidget-editbox
            input.form-control(type='text')
          .widget-body
            .table-responsive
              table.table.table-condensed.table-striped
                tbody
                  tr
                    th(style="width:160px") Cmd Count
                    td
                      span(data-sage="{'tlm':[{'name':'/CFS/DS/CmdAcceptedCounter'}]}") ---
                    td(style="width:100px")
                  tr
                    th Cmd Count
                    td
                      span(data-sage="{'tlm':[{'name':'/CFS/DS/CmdRejectedCounter'}]}") ---
                    td
                  tr
                    th Dest Tbl Load Count
                    td
                      span(data-sage="{'tlm':[{'name':'/CFS/DS/DestTblLoadCounter'}]}") ---
                    td
                  tr
                    th Dest Tbl Err Count
                    td
                      span(data-sage="{'tlm':[{'name':'/CFS/DS/DestTblErrCounter'}]}") ---
                    td
                  tr
                    th Filter Tbl Load Count
                    td
                      span(data-sage="{'tlm':[{'name':'/CFS/DS/FilterTblLoadCounter'}]}") ---
                    td
                  tr
                    th Filter Tbl Err Count
                    td
                      span(data-sage="{'tlm':[{'name':'/CFS/DS/FilterTblErrCounter'}]}") ---
                    td
                  tr
                    th App Enable State
                    td
                      span(style="width: 60px; display:inline-block", data-sage="{'tlm':[{'name':'/CFS/DS/AppEnableState'}]}") ---
                    td
                      .btn-group
                        button.btn.btn-xs.btn-primary.btn-labeled.sage-cmd-inline(data-sage="{'cmd': {'name':'/CFS/DS/SetAppState', 'argument':[{'name':'State', 'value':'Disabled'}]}}") Dis
                        button.btn.btn-xs.btn-primary.btn-labeled.sage-cmd-inline(data-sage="{'cmd': {'name':'/CFS/DS/SetAppState', 'argument':[{'name':'State', 'value':'Enabled'}]}}") Ena
                  tr
                    th File Write Count
                    td
                      span(data-sage="{'tlm':[{'name':'/CFS/DS/FileWriteCounter'}]}") ---
                    td
                  tr
                    th File Write Err Count
                    td
                      span(data-sage="{'tlm':[{'name':'/CFS/DS/FileWriteErrCounter'}]}") ---
                    td
                  tr
                    th File Update Count
                    td
                      span(data-sage="{'tlm':[{'name':'/CFS/DS/FileUpdateCounter'}]}") ---
                    td
                  tr
                    th File Update Err Count
                    td
                      span(data-sage="{'tlm':[{'name':'/CFS/DS/FileUpdateErrCounter'}]}") ---
                    td
                  tr
                    th Disabled Pkt Count
                    td
                      span(data-sage="{'tlm':[{'name':'/CFS/DS/DisabledPktCounter'}]}") ---
                    td
                  tr
                    th Ignored Pkt Count
                    td
                      span(data-sage="{'tlm':[{'name':'/CFS/DS/IgnoredPktCounter'}]}") ---
                    td
                  tr
                    th Filtered Pkt Count
                    td
                      span(data-sage="{'tlm':[{'name':'/CFS/DS/FilteredPktCounter'}]}") ---
                    td
                  tr
                    th Passed Pkt Count
                    td
                      span(data-sage="{'tlm':[{'name':'/CFS/DS/PassedPktCounter'}]}") ---
                    td
            footer
                .btn-group.dropup
                  button.btn.btn-primary.dropdown-toggle(type='button', data-toggle='dropdown')
                    | Commands
                    span.caret
                  ul#CSCommands.dropdown-menu(role='menu')
                    li
                      a(data-sage="{'cmd':{'name':'/CFS/DS/SetFilterFile'}}") Set Filter File
                    li
                      a(data-sage="{'cmd':{'name':'/CFS/DS/SetFilterType'}}") Set Filter Type
                    li
                      a(data-sage="{'cmd':{'name':'/CFS/DS/SetFilterParms'}}") Set Filter Params
                    li
                      a(data-sage="{'cmd':{'name':'/CFS/DS/SetDestType'}}") Set Dest Type
                    li
