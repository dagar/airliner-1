                    li
                      a(data-sage="{'cmd':{'name':'/CFS/DS/SetDestExt'}}") Set Dest Ext
                    li
                      a(data-sage="{'cmd':{'name':'/CFS/DS/SetDestSize'}}") Set Dest Size
                    li
