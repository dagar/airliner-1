                      span(data-sage="{'tlm': [{'name': '/CFS/CFE_EVS/CmdCounter'}]}") ---
                  tr
                    th CFE EVS Cmd Err Count
                    td
                      span(data-sage="{'tlm': [{'name': '/CFS/CFE_EVS/ErrCounter'}]}") ---
                  tr
                    th App Events
                    td
                      .btn-group
                        button.btn.btn-xs.btn-primary(data-sage="{'cmd': {'name':'/CFS/CFE_EVS/EnableAppEvents', 'argument':[{'name':'AppName', 'value':'VC'}]}}") Enable
                        button.btn.btn-xs.btn-primary(data-sage="{'cmd': {'name':'/CFS/CFE_EVS/DisableAppEvents', 'argument':[{'name':'AppName', 'value':'VC'}]}}") Disable
                        button.btn.btn-xs.btn-primary(data-sage="{'cmd': {'name':'/CFS/CFE_EVS/ResetAllFilters', 'argument':[{'name':'AppName', 'value':'VC'}]}}") Reset
                  tr
                    th Event Types
                    td
                  tr
                    td(style="padding-left: 20px !important") Critical
                    td
                      .btn-group
                        button.btn.btn-xs.btn-primary(data-sage="{'cmd': {'name':'/CFS/CFE_EVS/EnableAppEventType', 'argument':[{'name':'AppName', 'value':'VC'},{'name':'Critical', 'value':'Ignored'},{'name':'Error', 'value':'Ignored'},{'name':'Information', 'value':'Set'},{'name':'Debug', 'value':'Ignored'},{'name':'Spare', 'value':'0'}]}}") Enable
